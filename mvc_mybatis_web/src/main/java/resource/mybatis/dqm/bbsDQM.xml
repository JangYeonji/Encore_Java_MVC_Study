<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="encore.bbs">
		<select id="listRow" resultType="encore.bbs.model.vo.Bbs_vo">
			SELECT SEQ, SUBJECT, CONTENT, WRITTER, REGDATE, VIEWCNT
			FROM BBS_TBL
			ORDER BY SEQ DESC
		</select>
</mapper>































